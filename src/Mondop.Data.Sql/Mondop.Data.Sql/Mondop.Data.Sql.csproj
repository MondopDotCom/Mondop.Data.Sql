    <Copyright>Copyright 2020 by It-actief</Copyright>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>http://www.mondop.com</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MondopDotCom/Mondop.Data.Sql</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Mondop.Core" Version="1.2.2" />
    <PackageReference Include="Mondop.Expressions" Version="0.0.3-ALPHA001" />
